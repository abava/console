{
    "name": "venta/console",
    "description": "Venta eCommerce Framework Console package",
    "authors": [
        {
            "name": "Konstantins Zukovs",
            "email": "kz@scandiweb.com"
        },
        {
            "name": "Raivis Vitols",
            "email": "raivis@scandiweb.com"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ventacommerce/contracts.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/ventacommerce/extensions-loader.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Venta\\Console\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require-dev": {
        "phpunit/phpunit": "5.2"
    },
    "require": {
        "symfony/console": "3.0.*",
        "venta/contracts": "dev-master",
        "venta/extensions-loader": "dev-master"
    }
}
